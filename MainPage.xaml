<UserControl x:Class="TouchScreenComicViewer.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="Auto" d:DesignWidth="Auto">

    <Grid x:Name="LayoutRoot" Background="White" RenderTransformOrigin="0.5 0.5">
		<Grid.RenderTransform>
			<ScaleTransform x:Name="PageScale" ScaleX="1" ScaleY="1"/>
		</Grid.RenderTransform>
		<Image HorizontalAlignment="Center" Name="image1" VerticalAlignment="Top" Source="/TouchScreenComicViewer;component/Images/USM%20008%20-%2000%20Front%20Cover.jpg" />
	</Grid>
</UserControl>
